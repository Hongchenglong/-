.times {
  border: 1px solid darkgray;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  padding-left: 30rpx;
  line-height: 60rpx;
}

.table {
  margin: 30rpx 10rpx;
  background-color: #fff;
  border: 1px solid darkgray;
  font-size: 24rpx;
}

.tr {
  display: flex;
  width: 100%;
  height: 3rem;
}

.td,
.th {
  display: flex;
  justify-content: center;
  /*水平居中 */
  align-items: center;
  /*垂直居中*/
  width: 100%;
  border: 1px solid darkgray;
  height: 3rem;
}

.bg-w {
  background: #f2f2f2;
}