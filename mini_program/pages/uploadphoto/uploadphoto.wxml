<view class="wrap">
  <view class="contant_wrap">
    
    <view class="contant-pic">
      <view class="pics-list" wx:for="{{pics}}" wx:key="" >
       <image  src="{{item}}" class="uploadImg"></image>
       <image src="/images/error.png" class="uploadImg-error" data-img="{{item}}" bindtap='deleteImg'></image>
      </view>
      <image src="/images/upload.png" class="uploadImg {{isShow?'true':'hideTrue'}}" bindtap='uploadImage' ></image>
    </view>
  </view>
 <view >
<button type="primary" class="indexButton" bindtap="onLaunch" style="background-color:#00BFFF"> 确认提交 </button>
</view>
</view>