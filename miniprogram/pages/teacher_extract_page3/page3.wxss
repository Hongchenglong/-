.Section{
  font-size: 12px;
  margin: 10rpx 0rpx;
  width: 100%;
  height: 200rpx;
  line-height: 60rpx;
  border: solid 1px darkgray;
}

.whole,.Late,.start{
  display: flex;
  height: 50%;
  align-items: center;
  justify-content: center;  
  font-size: 32rpx;
  flex-direction: column;
}
image{
  width:23px;
  height:23px
}
.Start,.Later{
  margin-top: 50rpx;
  display: flex;
  flex-direction: row;
}
.Start{
  margin-top: 100rpx;
}
.text{
  margin-left: 20rpx;
}

.table {
  margin: 30rpx 10rpx;
  background-color: #fff;
  font-size: 12px;
  border: 1px solid darkgray;
  border-right: 0;
  border-bottom: 0;
}

.tr {
  display: flex;
  width: 100%;
  height: 3rem;
}

.td,.th {
  display: flex;
  justify-content: center;  /*水平居中 */
  align-items: center; /*垂直居中*/
  width: 100%;
  border-bottom: 1px solid darkgray;
  border-right: 1px solid darkgray;
  height: 3rem;
}

.bg-w{
  background: #f2f2f2;
}

.view-wrap{
  border: solid 1px;
  display: flex; 
  flex-direction: row;
}
